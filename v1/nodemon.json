{
  "restartable": "rs",
  "ignore": [".git", "./node_modules/**"],
  "watch": ["src"],
  "verbose": false,
  "env": {
    "PORT": 5000,
    "NODE_ENV": "development",
    "DB_ENABLE_LOGGING": "false",
    "ENABLE_GLOBAL_ERROR_LOGGING": "false"
  },
  "exec": "yarn run babel-node -x .ts,.js src",
  "ext": "ts"
}
